{
  "name": "nextjs-template",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx} --fix",
    "stylelint": "stylelint --ignore-path .gitignore ./src/**/*.{css,scss}",
    "stylelint:fix": "stylelint --ignore-path .gitignore ./src/**/*.{css,scss} --fix",
    "format": "prettier --ignore-path .gitignore --write --ignore-unknown .",
    "format:check": "prettier --ignore-path .gitignore --check --ignore-unknown .",
    "test": "jest --watch",
    "test:update": "jest -u",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn format"
    ],
    "src/**/*.{css,sass,scss}": [
      "yarn format"
    ]
  },
  "engines": {
    "node": "16.17.1",
    "npm": "Please use yarn instead"
  },
  "dependencies": {
    "destyle.css": "3.0.2",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@storybook/addon-a11y": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/builder-webpack5": "6.5.12",
    "@storybook/manager-webpack5": "6.5.12",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@trivago/prettier-plugin-sort-imports": "3.3.0",
    "@types/node": "17.0.18",
    "@types/react": "18.0.21",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "babel-loader": "8.2.5",
    "css-loader": "6.7.1",
    "eslint": "8.24.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-storybook": "0.6.4",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "sass": "1.55.0",
    "sass-loader": "13.0.2",
    "storybook-addon-next-router": "4.0.0",
    "style-loader": "3.3.1",
    "stylelint": "14.12.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.8.3"
  }
}
