{
  "name": "nextjs-template",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx} --fix",
    "stylelint": "stylelint --ignore-path .gitignore ./src/**/*.{css,scss}",
    "stylelint:fix": "stylelint --ignore-path .gitignore ./src/**/*.{css,scss} --fix",
    "format": "prettier --ignore-path .gitignore --write --ignore-unknown ./src/**/*.{js,jsx,ts,tsx}",
    "format:check": "prettier --ignore-path .gitignore --check --ignore-unknown ./src/**/*.{js,jsx,ts,tsx}",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn format"
    ],
    "src/**/*.{css,sass,scss}": [
      "yarn stylelint:fix",
      "yarn format"
    ]
  },
  "packageManager": "yarn@3.6.3",
  "volta": {
    "node": "18.17.1"
  },
  "dependencies": {
    "destyle.css": "4.0.0",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.11",
    "@storybook/addon-a11y": "7.3.2",
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/addon-mdx-gfm": "7.3.2",
    "@storybook/addon-styling": "^1.0.0",
    "@storybook/nextjs": "7.3.2",
    "@storybook/react": "7.3.2",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/node": "18.17.12",
    "@types/react": "18.2.21",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "babel-loader": "9.1.3",
    "css-loader": "6.8.1",
    "eslint": "8.48.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-storybook": "0.6.13",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.2",
    "sass": "1.66.1",
    "sass-loader": "13.3.2",
    "storybook": "7.3.2",
    "style-loader": "3.3.3",
    "stylelint": "15.10.3",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.3.0",
    "stylelint-config-standard-scss": "10.0.0",
    "stylelint-prettier": "4.0.2",
    "typescript": "5.2.2"
  }
}
