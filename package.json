{
  "name": "nextjs-template",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx} --fix",
    "stylelint": "stylelint --ignore-path .gitignore ./src/**/*.{css,scss}",
    "stylelint:fix": "stylelint --ignore-path .gitignore ./src/**/*.{css,scss} --fix",
    "format": "prettier --ignore-path .gitignore --write --ignore-unknown .",
    "format:check": "prettier --ignore-path .gitignore --check --ignore-unknown .",
    "test": "jest --watch",
    "test:update": "jest -u",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn format"
    ],
    "src/**/*.{css,sass,scss}": [
      "yarn format"
    ]
  },
  "engines": {
    "node": "18.14.2",
    "npm": "Please use yarn instead"
  },
  "dependencies": {
    "destyle.css": "4.0.0",
    "next": "13.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "babel-loader": "9.1.2",
    "css-loader": "6.7.3",
    "eslint": "8.35.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.2.2",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-storybook": "0.6.11",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "sass": "1.58.3",
    "sass-loader": "13.2.0",
    "storybook-addon-next-router": "4.0.2",
    "style-loader": "3.3.1",
    "stylelint": "15.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "3.1.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-prettier": "2.0.0",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.9.5"
  }
}
